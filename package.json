{
  "name": "zotero-paged-snapshots",
  "version": "0.1.0",
  "description": "Adds page buttons for navigating web snapshots.",
  "config": {
    "addonName": "Paged Snapshots",
    "addonID": "paged-snapshots@dylan.ac",
    "addonRef": "paged-snapshots",
    "addonInstance": "PagedSnapshots",
    "prefsPrefix": "extensions.zotero.paged-snapshots"
  },
  "scripts": {
    "dev": "zotero-plugin serve",
    "build": "zotero-plugin build && tsc --noEmit",
    "lint:check": "prettier --check . && eslint .",
    "lint:fix": "prettier --write . && eslint . --fix",
    "release": "zotero-plugin release",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Dylan Awalt-Conley",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^8.57.1",
    "@types/node": "^22.15.31",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^7.18.0",
    "zotero-plugin-scaffold": "^0.8.3",
    "zotero-types": "^4.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dawaltconley/zotero-paged-snapshots.git"
  },
  "keywords": [
    "zotero",
    "plugin"
  ],
  "bugs": {
    "url": "https://github.com/dawaltconley/zotero-paged-snapshots/issues"
  },
  "homepage": "https://github.com/dawaltconley/zotero-paged-snapshots#readme",
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "tabWidth": 2,
    "endOfLine": "lf",
    "overrides": [
      {
        "files": [
          "*.xhtml"
        ],
        "options": {
          "htmlWhitespaceSensitivity": "css"
        }
      }
    ]
  }
}
